<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
  xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
  jcr:primaryType="nt:unstructured"
  sling:resourceType="kestros/cms/dialog"
  jcr:title="Edit">
  <content-area jcr:primaryType="nt:unstructured">
    <tabs
      jcr:primaryType="nt:unstructured"
      sling:resourceType="kestros/cms/general/tabs"
      container="true">
      <general jcr:primaryType="nt:unstructured"
        sling:resourceType="kestros/cms/general/container"
        jcr:title="General"
        container="true">
        <text
          jcr:primaryType="nt:unstructured"
          sling:resourceType="kestros/cms/fields/textfield"
          label="Text"
          propertyName="text"
          autofocus="true"/>
        <heading-level
          jcr:primaryType="nt:unstructured"
          sling:resourceType="kestros/cms/fields/radio-list-field"
          label="Heading Level"
          propertyName="level">
          <options jcr:primaryType="nt:unstructured">
            <h1 jcr:primaryType="nt:unstructured"
              text="h1"
              value="h1"
              initialValue="true"/>
            <h2 jcr:primaryType="nt:unstructured"
              text="h2"
              value="h2"/>
            <h3 jcr:primaryType="nt:unstructured"
              text="h3"
              value="h3"/>
            <h4 jcr:primaryType="nt:unstructured"
              text="h4"
              value="h4"/>
            <h5 jcr:primaryType="nt:unstructured"
              text="h5"
              value="h5"/>
            <h6 jcr:primaryType="nt:unstructured"
              text="h6"
              value="h6"/>
          </options>
        </heading-level>
      </general>
    </tabs>
  </content-area>
</jcr:root>
